    <string name="pg1_blktle">"NVIDIA-TASTE"</string>
    <add-resource type="string" name="pg1_blktxt" />
    <string name="pg1_blktxt">"<b>Einmal drücken</b>, um SHIELD-Spiele zu starten, neue Spiele zu finden und Spiele von Ihrem GeForce GTX PC zu streamen.\n\n<b>Gedrückt halten</b>, um das Akku-Menü zu öffnen."</string>
    <add-resource type="string" name="pg2_voltle" />
    <string name="pg2_voltle">"LAUTSTÄRKE"</string>
    <add-resource type="string" name="pg2_voltxt" />
