<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Reason displayed when installation fails because the package specifies it is compatible
        only with a CPU that the current SHIELD doesn't have. [CHAR LIMIT=100] -->
    <string name="install_failed_cpu_abi_incompatible" product="shield">This app isn\'t
        compatible with SHIELD.</string>
    <!-- Message presented when an application could not be installed on the SHIELD for some reason. [CHAR LIMIT=100] -->
    <string name="install_failed_msg" product="shield"><xliff:g id="app_name">%1$s</xliff:g> couldn\'t be installed on SHIELD.</string>
   <!-- Message presented in a dialog box when a package is requested to be installed, but the "Allow unknown applications" checkbox is not enabled. [CHAR LIMIT=100] -->
    <string name="unknown_apps_dlg_text" product="shield">For security, SHIELD is set to block installation of apps obtained from unknown sources.</string>
</resources>
